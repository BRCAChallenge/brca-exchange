        <script>
            (function() {
                var listeners = [], loaded = false;
                window.onRecaptchaLoad = callback => (loaded ? callback() : listeners.push(callback));
                window.recaptchaCallback = function() {
                    loaded = true;
                    while (listeners.length) {
                        listeners.pop()();
                    }
                };
            })();
        </script>
